{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 0,
        "quotes": ["error", "single"]
    },
    "globals": {
        "document": false
    }
}